
@{
    ViewBag.Title = "Registration Success";
}

<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
<form id="emailForm" method="post">
    <div class="container">
        <div class="card">
            <div class="card-header text-center"><h3>Personal Details</h3></div>
            <div class="card-body">
                <div class="row text-center d-flex">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                        <p><strong>Name:</strong> @TempData["Name"]</p>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                        <p><strong>Date of Birth:</strong> @TempData["DOB"]</p>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                        <p><strong>PAN:</strong> @TempData["PAN"]</p>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                        <p><strong>Mobile Number:</strong> @Session["MobileNumber"]</p>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
                        <p><strong>Email:</strong> @Session["Email"]</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
